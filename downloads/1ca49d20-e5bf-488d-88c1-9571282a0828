    <script src="https://ajax.googleapis.com/ajax/libs/jquery/1.11.2/jquery.min.js"></script>
    <script src="/App_Themes/RAPS/assets/dist/js/easy-responsive-tabs.js"></script>
    <script>
        jQuery(document).ready(function ($) {
            if ($("#p_lt_ctl01_Header_PageID").text() == '1b17457e-e0a5-4e47-bbce-57218ff03bd8') {
                $("#topbanner").show();
                $("#menuitems").show();
                $("#menu-li-img").show();
                $("#header_new").css("border-bottom", "1px #bbb solid");
            }
            else {
                if ($(window).width() > 1145) {
                    $("#topbanner").hide();
                    $("#menuitems").hide();
                    $("#menu-li-img").hide();
                } else {
                    $("#topbanner").hide();
                    $("#menuitems").hide();
                    $('.menu-li-img').css('display', 'inline-block');
                }
                $(window).resize(function () {
                    if ($(window).width() > 1145) {
                        $("#topbanner").hide();
                        $("#menuitems").hide();
                        $("#menu-li-img").hide();
                    } else {
                        $("#topbanner").hide();
                        $("#menuitems").hide();
                        $('.menu-li-img').css('display', 'inline-block');
                    }
                });
            }
            if (window.location.href.toString().indexOf('/news-articles/') > 0) { 
                $("#topbanner").show();
            }
            $('#vertical_menu').easyResponsiveTabs({
                type: 'vertical',
                width: 'auto',
                fit: true,
                closed: 'tabs'
            });
        });

        function toggleMenu() {
            var menuDiv = document.getElementById("menuDiv");
            if ((menuDiv.style.display === "none") || (menuDiv.style.display === "")) {
                menuDiv.style.display = "block";
                document.getElementById('menu_button').classList.add('active-menu-button');
            } else {
                menuDiv.style.display = "none";
                document.getElementById('menu_button').classList.remove('active-menu-button');
            }
        }

        function togglePopup() {
            var popupDiv = document.getElementById("menu-pop");
            if ((popupDiv.style.display === "none") || (popupDiv.style.display === "")) {
                if ($(window).width() > 1150) {
                    $('#menu-pop').slideToggle();
                    popupDiv.style.display = "flex";
                } else {
                    $('#menu-pop').slideToggle();
                    popupDiv.style.display = "block";
                }
                $(window).resize(function () {
                    //if ($(window).width() > 1150) {
                    //    $('#menu-pop').slideDown();
                    //    popupDiv.style.display = "flex";
                    //} else {
                    //    $('#menu-pop').slideDown();
                    //    popupDiv.style.display = "block";
                    //}
                });
            } else {
                $('#menu-pop').slideToggle();
                //popupDiv.style.display = "none";
            }
        }

        function closeMenu() {
            $('#menu-pop').slideToggle();
        }
        
        $(document).keyup(function(e) {
            if (e.keyCode === 27) $('#menu-pop').hide();   // esc key
        });
    </script>
</header>
